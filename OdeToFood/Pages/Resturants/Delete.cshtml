@page "{resturantId}"
@model OdeToFood.Pages.Resturants.DeleteModel
@{
    ViewData["Titel"] = "Delete";
}



    <h2>Delete!</h2>
    <div class="alert alert-danger">
        Are you sure you want to delete @Model.Resturant.Name?
    </div>
    <form method="post">
        <button type="submit" class="btn btn-danger">Yes!</button>
        <a asp-page="List" class="btn btn-default">Cancel</a>
    </form>

